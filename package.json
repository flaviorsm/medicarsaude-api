{
  "name": "medicar-saude-api",
  "version": "1.0.0",
  "description": "Sistema de cooperativa de saúde",
  "engines": {
    "node": "14.15.1"
  },
  "main": "index.ts",
  "scripts": {
    "start": "node dist/api.bundle.js",
    "dev": "tsc ./src/index.ts --outDir ./build && node src/config/swagger.js && node ./build/index.js",
    "dev:watch": "nodemon ./build/index.js",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "tslint-fix": "tslint --project tsconfig.json --config tslint.json --fix",
    "build": "webpack",
    "swagger-autogen": "tsc ./src/index.ts --outDir ./build && node src/config/swagger.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
