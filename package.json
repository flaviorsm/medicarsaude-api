    "mongodb": "^4.3.1",
    "mongoose": "^6.1.10",
    "pine": "^1.1.1",
    "swagger-autogen": "^2.18.8",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.11.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.15",
